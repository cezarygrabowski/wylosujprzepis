{% extends "@FOSUser/Profile/show.html.twig" %}
{% trans_default_domain 'FOSUserBundle' %}
{% block profileContent %}

<div id="registerForm" class="user_register">
    {{ form_start(form, { 'action': path('fos_user_profile_edit'), 'attr': { 'class': 'fos_user_profile_edit' } }) }}
    {{ form_widget(form) }}
    <div class="action_btns top-padding">
        <input type="submit" class="btn btn_red" id="_submit" name="_submit" value="{{ 'Zaktualizuj dane'|trans }}" />
    </div>
    {{ form_end(form) }}
</div>
{% endblock %}

